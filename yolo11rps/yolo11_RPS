import cv2
from ultralytics import YOLO


model = YOLO('best.pt')

cap = cv2.VideoCapture(0)

while True:
    ret,frame = cap.read()
    if not ret:
        break

    results = model.predict(source=frame, device='cpu', conf=0.5, imgsz=416, stream=False, verbose=False)

    annotated_frame = results[0].plot()
    cv2.imshow('YOLO WEBCAM', annotated_frame)
    

    if cv2.waitKey(1)  == ord('q'):
        break


cap.release()
cv2.destroyAllWindows()    




